   "source": [
    "### **Checking the time a file was created and last modified** "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def timestamp(path):\n",
    "    import os\n",
    "    import time\n",
    "    print(\"Created: %s\" % time.ctime(os.path.getctime(path)))\n",
    "    print(\"Last modified: %s\" % time.ctime(os.path.getmtime(path)))\n",
    "   \n",
    "timestamp(\"[FILEPATH]\")"
   ]
  }
 ],
